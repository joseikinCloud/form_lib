{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "allObj": "readonly",
        "$": "readonly",
        "jQuery": "readonly",
        "Encoding": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2023,
        "sourceType": "module"
    },
    "rules": {
        "max-len": [
            "error",
            {
                "code": 1024
            }
        ],
        "max-classes-per-file": [
            "error",
            {
                "ignoreExpressions": true,
                "max": 100
            }
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "no-console": "off",
        "arrow-parens": [
            "error",
            "as-needed"
        ],
        "arrow-body-style": [
            "error",
            "as-needed"
        ],
        "curly": [
            "error",
            "multi"
        ],
        "func-names": [
            "error",
            "as-needed"
        ],
        "no-use-before-define": "off",
        "no-param-reassign": [
            "error",
            {
                "props": false
            }
        ],
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "^_$"
            }
        ],
        "lines-between-class-members":"off",
        "no-prototype-builtins":"off"
    }
}
